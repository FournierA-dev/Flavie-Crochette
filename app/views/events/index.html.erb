<section id="events-list", class="white-wrapper ">

  <%= render partial: 'components/titlePage',locals: {title:'Nos prochains événements',image:'logos/2.jpg'}%>

    <div class="container ">
      <div class="light-wrapper p-5">
        <div class="row  mx-auto">
          <% @events.each do |event| %>
            <div class="col-sm-4  mb-3 ">
              <%= render partial:'components/eventCard',locals:{event: event} %>
            </div>
          <% end %>
        </div>
      </div>
    </div>

</section>

